{
  "name": "dragonbones-pixijs",
  "version": "1.0.3",
  "description": "DragonBones runtime for pixi v7,v8+",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/dragonbones-pixi.js",
      "require": "./dist/dragonbones-pixi.js"
    }
  },
  "files": [
    "dist"
  ],
  "main": "dist/dragonbones-pixi.umd.cjs",
  "module": "dist/dragonbones-pixi.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SGGames/DragonBones-Pixi.git"
  },
  "homepage": "https://github.com/SGGames/DragonBones-Pixi#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean": "tsc --build --clean",
    "build-demo": "vite build --config vite.demo.config.js",
    "build-demo-ghp": "vite build --base=/DragonBones-Pixi/ --config vite.demo.config.js",
    "surge": "surge dist-demo dragonbones-pixi-v8-demo.surge.sh"
  },
  "keywords": [
    "dragonbones",
    "pixi"
  ],
  "author": "atomixnmc",
  "license": "ISC",
  "dependencies": {
    "pixi.js": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "eslint": "^9.1.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "vite": "^5.2.0"
  }
}
